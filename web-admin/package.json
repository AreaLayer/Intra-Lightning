{
  "name": "web-admin",
  "version": "0.2.0",
  "homepage": "http://localhost:5401/",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@l2-technology/sensei-client": "0.1.25",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-plugin-formatjs": "^10.5.2",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.30.0",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.1.0",
    "final-form-calculate": "^1.3.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "prettier": "^2.8.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-final-form-listeners": "^1.0.3",
    "react-intl": "^6.4.4",
    "react-popper": "^2.3.0",
    "react-qr-code": "^2.0.11",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "typescript": "^5.1.3",
    "web-vitals": "^3.3.2",
    "zod": "^3.21.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "cross-env PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "intl:extract": "formatjs extract --throws 'src/**/*.{ts,tsx}' --ignore 'src/**/*.{d,test}.{ts,tsx}' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin --out-file ./lang/en.json",
    "intl:compile": "formatjs compile-folder --format crowdin --ast ./lang src/translations"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "destructuredArrayIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^6.1.3",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  }
}
